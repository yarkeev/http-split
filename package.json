{
	"name": "http-split",
	"version": "1.0.6",
	"description": "split http services",
	"main": "dist/index.js",
	"scripts": {
		"ts": "./node_modules/.bin/tsc",
		"build": "rm -rf ./dist && npm run ts",
		"dev": "./node_modules/.bin/tsc -w & supervisor -- ./bin/run --workers=1",
		"prepublish": "npm run build && bump"
