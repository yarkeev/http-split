	"license": "MIT",
	"devDependencies": {
		"@types/express": "^4.16.1",
		"@types/http-proxy": "^1.17.0",
		"@types/node": "^12.0.2",
		"supervisor": "^0.12.0",
		"typescript": "^3.4.5",
		"version-bump-prompt": "^5.0.1"
	},
	"dependencies": {
		"commander": "^2.20.0",
		"express": "^4.17.0",
		"http-proxy": "^1.17.0"
	}
}
